{
  "manifest_version": 2,
  "name": "WebToEpub",
  "version": "0.0.0.144",
  "default_locale": "en",
  "icons": {
    "128": "book128.png"
  },
  "permissions": [
    "downloads",
    "<all_urls>",
    "*://*/*",
    "tabs"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "browser_style": false,
    "default_title": "",
    "default_icon": "book128.png"
  },
  "incognito": "spanning",
  "applications": {
    "gecko": {
      "id": "WebToEpub@Baka-tsuki.org",
      "strict_min_version": "79.0"
    }
  },
  "minimum_chrome_version": "80"
}
