{
  "manifest_version": 3,
  "name": "WebToEpub",
  "version": "0.0.0.151",
  "default_locale": "en",
  "icons": {
    "128": "book128.png"
  },
  "permissions": [
    "downloads",
    "webRequest",
    "webRequestBlocking",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "browser_style": false,
    "default_title": "",
    "default_icon": "book128.png",
    "default_popup": "popup.html"
  },
  "incognito": "split",
  "applications": {
    "gecko": {
      "id": "WebToEpub@Baka-tsuki.org",
      "strict_min_version": "79.0"
    }
  },
  "minimum_chrome_version": "88"
}
